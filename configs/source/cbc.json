      ["length", "u32"],
      ["validator_count", "u32"]
    ]
  },
  "EpochHistory": {
    "type": "struct",
    "type_mapping": [
      ["epoch", "u32"],
      ["start_block", "u32"],
      ["end_block", "u32"],
      ["validator_count", "u32"],
      ["total_stake", "u128"],
      ["average_score", "u64"],
      ["blocks_produced", "u32"],
      ["total_rewards", "u128"]
    ]
  },
  "SystemMetrics": {
    "type": "struct",
    "type_mapping": [
      ["total_validators", "u32"],
      ["active_validators", "u32"],
      ["total_stake", "u128"],
      ["average_score", "u64"],
      ["current_epoch", "u32"],
      ["blocks_in_epoch", "u32"],
      ["epoch_progress", "u32"]
    ]
  },
  "AuthorSelectionMode": {
    "type": "enum",
    "value_list": ["RoundRobin", "StakeWeighted", "PerformanceBased", "Hybrid"]
  },
  "ConsensusConfig": {
    "type": "struct",
    "type_mapping": [
      ["pos_weight", "u64"],
      ["poi_weight", "u64"],
      ["epoch_length", "u32"],
      ["max_validators", "u32"],
      ["min_stake", "Balance"]
    ]
  },
  "InferenceChallenge": {
    "type": "struct",
    "type_mapping": [
      ["challenge_id", "u32"],
      ["challenger", "AccountId"],
      ["target_validator", "AccountId"],
      ["challenge_data", "Vec<u8>"],
      ["expected_result", "Vec<u8>"],
      ["deadline", "BlockNumber"],
      ["is_resolved", "bool"]
    ]
  },
  "ChallengeResult": {
    "type": "struct",
    "type_mapping": [
      ["challenge_id", "u32"],
      ["validator", "AccountId"],
      ["result", "Vec<u8>"],
      ["submission_block", "BlockNumber"],
      ["is_correct", "bool"]
    ]
  },
  "ValidatorPerformance": {
    "type": "struct",
    "type_mapping": [
      ["validator", "AccountId"],
      ["blocks_produced", "u32"],
      ["blocks_expected", "u32"],
      ["uptime_percentage", "u32"],
      ["challenges_completed", "u32"],
      ["challenges_failed", "u32"],
      ["last_updated", "BlockNumber"]
    ]
  },
  "InvariantViolation": {
    "type": "enum",
    "type_mapping": [
      ["Economic", "Vec<u8>"],
      ["Validator", "Vec<u8>"],
      ["Temporal", "Vec<u8>"]
    ]
  },
  "InvariantSeverity": {
    "type": "enum",
    "value_list": ["Low", "Medium", "High", "Critical"]
  },
  "ParameterType": {
    "type": "enum",
    "value_list": [
      "PosWeight", "PoiWeight", "MinStake", "MaxValidators", "EpochLength",
      "MinValidatorScore", "MaxValidatorScore", "ValidatorScoreDecay",
      "SlashingPenalty", "RewardRate", "TrustScoreUptimeWeight",
      "TrustScoreInferenceWeight", "TrustScoreSlashingWeight"
    ]
  },
  "DeterministicEpochEngine": {
    "type": "struct",
    "type_mapping": [
      ["randomness_salt", "[u8; 32]"],
      ["epoch_salt", "[u8; 16]"],
      ["author_sequence_cache", "Vec<u8>"],
      ["last_processed_epoch", "u32"],
      ["last_epoch_output_hash", "[u8; 32]"]
    ]
  },
  "SessionKeys": {
    "type": "struct",
    "type_mapping": [
      ["dcf", "[u8; 32]"]
    ]
  }
}