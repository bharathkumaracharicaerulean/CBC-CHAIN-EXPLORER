{
  "Address": "AccountId",
  "BlockNumber": "u32",
  "Balance": "u128",
  "Weight": "u64",
  "Keys": "SessionKeys",
  "Signature": "MultiSignature",
  "Hash": "H256",
  "Nonce": "u32",
  "DispatchInfo": {
    "type": "struct",
    "type_mapping": [
      ["weight", "Weight"],
      ["class", "DispatchClass"],
      ["paysFee", "Pays"]
    ]
  },
  "DispatchResult": {
    "type": "enum",
    "type_mapping": [
      ["Ok", "Null"],
      ["Error", "DispatchError"]
    ]
  },
  "EpochStats": {
    "type": "struct",
    "type_mapping": [
      ["epoch", "u32"],
      ["stake_score", "u64"],
      ["inference_score", "u64"],
      ["final_score", "u64"],
      ["authored_blocks", "u32"],
      ["missed_blocks", "u32"]
    ]
  },
  "ValidatorState": {
    "type": "struct",
    "type_mapping": [
      ["last_active_epoch", "u32"],
      ["current", "EpochStats"],
      ["history", "Vec<EpochStats>"],
      ["uptime", "u32"],
      ["inference_success_count", "u32"],
      ["participation_rate", "u32"],
      ["inference_count", "u64"],
      ["last_active_block", "u32"],
      ["name", "Option<Vec<u8>>"],
      ["trust_score", "u64"]
    ]
  },
  "EpochConfig": {
    "type": "struct",
    "type_mapping": [
      ["blocks_per_epoch", "u32"],
      ["min_stake", "u128"],
      ["max_validators", "u32"]
    ]
  },
  "TrustScoreConfig": {
    "type": "struct",
    "type_mapping": [
      ["uptime_weight", "u32"],
      ["inference_weight", "u32"],
      ["slashing_weight", "u32"],
      ["max_growth_rate", "u32"],
      ["max_decay_rate", "u32"],
      ["min_trust_score", "u64"],
      ["max_trust_score", "u64"],
      ["stability_factor", "u32"]
    ]
  },
  "TrustScoreBoundsData": {
    "type": "struct",
    "type_mapping": [
      ["min_score", "u64"],
      ["max_score", "u64"],
      ["max_growth_rate", "u32"],
      ["max_decay_rate", "u32"],
      ["stability_factor", "u32"],
      ["last_updated_epoch", "u32"]
    ]
  },
  "ProposalAction": {
    "type": "enum",
    "type_mapping": [
      ["Slash", "(AccountId, u128)"],
      ["Reward", "(AccountId, u128)"],
      ["Eject", "AccountId"],
      ["AddValidator", "AccountId"],
      ["RemoveValidator", "AccountId"],
      ["RewardMultiple", "Vec<(AccountId, u128)>"]
    ]
  },
  "GovernanceProposal": {
    "type": "struct",
    "type_mapping": [
      ["id", "u32"],
      ["proposer", "AccountId"],
      ["action", "ProposalAction"],
      ["votes_for", "u32"],
      ["votes_against", "u32"],
      ["status", "ProposalStatus"],
      ["created_at", "u32"]
    ]
  },
  "ProposalStatus": {
    "type": "enum",
    "value_list": ["Pending", "Approved", "Rejected", "Executed"]
  },
  "ValidatorAction": {
    "type": "enum",
    "type_mapping": [
      ["Join", "u128"],
      ["Leave", "Null"]
    ]
  },
  "ValidatorStatus": {
    "type": "enum",
    "value_list": ["Active", "Inactive", "Leaving", "Cooldown"]
  },
  "ValidatorMetadataInfo": {
    "type": "struct",
    "type_mapping": [
      ["name", "Option<Vec<u8>>"],
      ["stake", "u128"],
      ["score", "u64"],
      ["trust_score", "u64"],
      ["uptime", "u32"],
      ["participation_rate", "u32"],
      ["blocks_authored", "u32"],
      ["blocks_missed", "u32"],
      ["inference_count", "u64"],
      ["last_active_block", "u32"],
      ["join_time", "u32"],
      ["status", "ValidatorStatus"]
    ]
  },
  "PerformanceRecord": {
    "type": "struct",
    "type_mapping": [
      ["epoch", "u32"],
      ["blocks_authored", "u32"],
      ["blocks_missed", "u32"],
      ["inference_count", "u32"],
      ["score", "u64"],
      ["participation_rate", "u32"]
    ]
  },
  "ValidatorProfile": {
    "type": "struct",
    "type_mapping": [
      ["account_id", "AccountId"],
      ["stake", "Balance"],
      ["inference_capability", "u32"],
      ["pos_performance_score", "u32"],
      ["poi_performance_score", "u32"],
      ["trust_score", "u64"],
      ["is_active", "bool"],
      ["last_active_epoch", "u32"]
